    "@mdi/js": "^7.2.96",
    "@popperjs/core": "^2.11.8",
    "chart.js": "^4.4.0",
    "electron-updater": "^6.1.1",
    "tw-elements": "^1.0.0",
    "v-calendar": "^3.1.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.3.1",
